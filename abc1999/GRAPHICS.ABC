CASE ASC(extcode)„ CASE 10„  app0error% = 0„  PRINT "Ignoring thumbnail image coded using JPEG."„  SEEK #1, SEEK(1) + length% - 7„  EXIT SUB„ CASE 11„  app0error% = 0„  PRINT "Ignoring thumbnail image coded using 1 byte/pixel."„  SEEK #1, SEEK(1) + length% - 7„  EXIT SUB„ CASE 12„  app0error% = 0„  PRINT "Ignoring thumbnail image coded using 3 bytes/pixel."„  SEEK #1, SEEK(1) + length% - 7„  EXIT SUB„ CASE ELSE„  EXIT SUB„ END SELECT„ELSE„ app0error% = 0„ PRINT "Ignoring unknown APP0 segment."„ SEEK #1, SEEK(1) + length% - 7„ EXIT SUB„END IF„„GET #1, , jpeg.version„„GET #1, , units„jpeg.units = ASC(units)„„GET #1, , density„jpeg.xden = ASC(LEFT$(density, 1)) * &H100 + ASC(RIGHT$(density, 1))„GET #1, , density„jpeg.yden = ASC(LEFT$(density, 1)) * &H100 + ASC(RIGHT$(density, 1))„„GET #1, , readthumbs„jpeg.xthumb = ASC(readthumbs)„GET #1, , readthumbs„jpeg.ythumb = ASC(readthumbs)„„IF jpeg.xthumb * jpeg.ythumb > 0 THEN PRINT "Ignoring 24-bit thumbnail image."„SEEK #1, SEEK(1) + 3 * jpeg.xthumb * jpeg.ythumb„„app0error% = 0„„END SUB„„SUB ReadCOM„„DIM lengthval AS STRING * 2„DIM comdata AS STRING * 1„„GET #1, , lengthval„length% = (ASC(LEFT$(lengthval, 1)) * &H100 + ASC(RIGHT$(lengthval, 1)))„„PRINT "Commentary marker found. View marker data? (Y/N)"„PRINT„„DO„   keypress$ = UCASE$(INKEY$)„   IF keypress$ = "N" THEN„    SEEK #1, SEEK(1) + length% - 2„    EXIT SUB„   END IF„LOOP UNTIL keypress$ = "Y"„„FOR viewdata% = 1 TO length% - 2„   GET #1, , comdata„   PRINT comdata;„NEXT viewdata%„„PRINT„PRINT„PRINT "Press any key to continue..."„DO: LOOP WHILE INKEY$ = ""„„END SUB„„SUB ReadDHT (dhterror%)„SHARED jpeg AS JPEGType„SHARED huffval%(), huffsize%(), huffcode&()„„dhterror% = 5„„DIM lengthval AS STRING * 2„DIM tcth AS STRING * 1„DIM ncodes AS STRING * 1„DIM incode AS STRING * 1„„GET #1, , lengthval„length% = ASC(LEFT$(lengthval, 1)) * &H100 + ASC(RIGHT$(lengthval, 1)) - 2„„DO„   REDIM bits%(1 TO 16)„„   GET #1, , tcth„   tableclass% = ASC(tcth) \ &H10„   tableid% = ASC(tcth) AND &HF„„   vallength% = 0„   FOR getnumcodes% = 1 TO 16„      GET #1, , ncodes„      bits%(getnumcodes%) = ASC(ncodes)„      vallength% = vallength% + ASC(ncodes)„   NEXT getnumcodes%„„   valofs% = 0„   FOR scancodes% = 1 TO 16„      FOR getcodes% = 1 TO bits%(scancodes%)„         GET #1, , incode„         huffval%(tableclass%, tableid%, valofs%) = ASC(incode)„         valofs% = valofs% + 1„      NEXT getcodes%„   NEXT scancodes%„„   k% = 0„   j% = 1„   FOR i% = 1 TO 16„      WHILE j% <= bits%(i%)„         huffsize%(tableclass%, tableid%, k%) = i%„         k% = k% + 1„         j% = j% + 1„      WEND„      j% = 1„   NEXT i%„   huffsize%(tableclass%, tableid%, k%) = 0„„   k% = 0„   code& = 0„   si% = huffsize%(tableclass%, tableid%, 0)„   DO„      DO„         huffcode&(tableclass%, tableid%, k%) = code&„         code& = code& + 1„         k% = k% + 1„      LOOP WHILE huffsize%(tableclass%, tableid%, k%) = si%„„      IF huffsize%(tableclass%, tableid%, k%) = 0 THEN EXIT DO„„      DO„         code& = code& * 2„         si% = si% + 1„      LOOP UNTIL huffsize%(tableclass%, tableid%, k%) = si%„   LOOP WHILE huffsize%(tableclass%, tableid%, k%) = si%„„   length% = length% - 17 - vallength%„LOOP UNTIL length% = 0„„dhterror% = 0„„END SUB„„SUB ReadDNL (dnlerror%)„„END SUB„„SUB ReadDQT (dqterror%)„SHARED jpeg AS JPEGType„SHARED dqttable%()„„dqterror% = 3„„DIM lengthval AS STRING * 2„DIM pt AS STRING * 1„DIM dqtvalue AS STRING * 1„„GET #1, , lengthval„length% = ASC(LEFT$(lengthval, 1)) * &H100 + ASC(RIGHT$(lengthval, 1)) - 3„„DO„   GET #1, , pt„   precision% = ASC(pt) \ &H10„   tableid% = ASC(pt) AND &HF„„   FOR readtable% = 0 TO 63„      GET #1, , dqtvalue„      dqttable%(tableid%, readtable%) = ASC(dqtvalue)„   NEXT readtable%„„   length% = length% - 64„   jpeg.numdqts = jpeg.numdqts + 1„LOOP UNTIL length% = 0„„dqterror% = 0„„END SUB„„SUB ReadDRI„SHARED jpeg AS JPEGType„„DIM ri AS STRING * 2„„GET #1, , length%„„GET #1, , ri„jpeg.ri = ASC(LEFT$(ri, 1)) * &H100 + ASC(RIGHT$(