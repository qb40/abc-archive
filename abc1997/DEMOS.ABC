) > 0)ã         CASE ELSE: falling = ((x% MOD 10) > 2)ã      END SELECTã   END IFã   SELECT CASE INP(96)ã      CASE 75: move$ = "RL"ã      CASE 203: move$ = "SL"ã      CASE 77: move$ = "RR"ã      CASE 205: move$ = "SR"ã      CASE 72: move$ = "CU"ã      CASE 200: move$ = "SB"ã      CASE 80: move$ = "CD"ã      CASE 208: move$ = "SB"ã      CASE 82: move$ = "SF"ã   END SELECTã   SELECT CASE move$ã      CASE "RL"ã         w% = -1: IF y% > 9 THEN w% = (ASC(MID$(levdat, xy% - 1)) = 6)ã         IF ((y% MOD 10) <> 0) OR (NOT w%) THENã            IF falling THEN nx% = nx% + 1ã            ny% = ny% - 1: figure = runleft + lr% \ 4ã            lr% = (lr% + 1) MOD 12: GOSUB blitplayerã         ELSEã            IF figure <> standleft THENã               figure = standleft: GOSUB blitplayerã            END IF: move$ = "SL"ã         END IFã      CASE "SL"ã         IF figure <> standleft THENã            figure = standleft: IF falling THEN nx% = nx% + 1ã            GOSUB blitplayerã         ELSEã            IF falling THEN nx% = nx% + 1: GOSUB blitplayerã         END IFã      CASE "RR"ã         w% = -1: IF y% < 310 THEN w% = (ASC(MID$(levdat, xy% + 1)) = 6)ã         IF ((y% MOD 10) <> 0) OR (NOT w%) THENã            IF falling THEN nx% = nx% + 1ã            ny% = ny% + 1: figure = runright + RR% \ 4ã            RR% = (RR% + 1) MOD 12: GOSUB blitplayerã         ELSEã            IF figure <> standright THENã               figure = standright: GOSUB blitplayerã            END IF: move$ = "SR"ã         END IFã      CASE "SR"ã         IF figure <> standright THENã            figure = standright: IF falling THEN nx% = nx% + 1ã            GOSUB blitplayerã         ELSEã            IF falling THEN nx% = nx% + 1: GOSUB blitplayerã         END IFã      CASE "CU"ã         IF x% > 0 THENã            O% = ASC(MID$(levdat, xy%)): O% = (O% = 4) + (O% = 1)ã         END IFã         IF x% < 608 THENã            b% = ASC(MID$(levdat, xy% + 32)): b% = (b% = 1) + (b% = 4)ã         END IF: L% = b% + O%ã         IF L% THENã            figure = climb + CL% \ 5: CL% = (CL% + 1) MOD 15ã            nx% = nx% + (nx% > 0)ã            ny% = ny% + 3 - ((ny% + 3) MOD 10): GOSUB blitplayerã         ELSEã            move$ = "SB"ã            IF figure <> back THEN figure = back: GOSUB blitplayerã         END IFã      CASE "CD"ã         IF (x% MOD 10) <> 0 THENã            b% = ASC(MID$(levdat, xy% + 32)): O% = ASC(MID$(levdat, xy%))ã            IF (O% = 4) OR (O% = 5) OR (b% = 4) OR (b% = 5) THENã               figure = climb + CL% \ 5: CL% = (CL% + 14) MOD 15ã               nx% = nx% + 1: ny% = (ny% + 3) - ((ny% + 3) MOD 10)ã               GOSUB blitplayerã            END IFã         ELSEã            b% = ASC(MID$(levdat, xy% + 32))ã            IF ((b% = 2) OR (b% = 4) OR (b% = 5)) AND (x% < 180) THENã               figure = climb + CL% \ 5: CL% = (CL% + 14) MOD 15ã               nx% = nx% + 1: ny% = (ny% + 3) - ((ny% + 3) MOD 10)ã               GOSUB blitplayerã            END IFã         END IFã      CASE "SB": IF falling THEN nx% = nx% + 1: figure = frontã         IF figure <> back THEN figure = back: GOSUB blitplayerã      CASE "SF": IF falling THEN nx% = nx% + 1: figure = backã         IF figure <> front THEN figure = front: GOSUB blitplayerã   END SELECT: DEF SEG = &H40: sh% = PEEK(&H17): DEF SEGã   IF ((sh% AND 8) = 8) AND (INSTR("SLSR", move$) > 0) THENã      blast = sprite(0): Yloc% = 10: fire% = shootrightã      IF move$ = "SL" THEN Yloc% = -10: fire% = shootleftã      blast.y = blast.y + Yloc%: wave% = (wave% + 1) AND 1ã      WAIT &H3DA, 8: WAIT &H3DA, 8, 8: SOUND RND * 1000 + 100, .3ã      blast.sprite = weapon(fire% + wave%): qbblitsprite blastã      WAIT &H3DA, 8: WAIT &H3DA, 8, 8: SOUND RND * 1000 + 999, .3ã      blast.sprite = blast.buffer: qbputsprite blastã   END IFã   FOR sp% = 1 TO 5: qbputsprite sprite(sp%)ã      sprite(sp%).sprite = monster(objects(sp% - 1).objdef)ã      qbblitsprite sprite(sp%)ã      sprite(sp%).sprite = sprite(sp%).buffer: NEXTãNEXT: DEF