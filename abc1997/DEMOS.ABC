IF met = 4 THENã IF POINT(i%, j%) = 15 THENã  LINE (x1 + i% * 4, y1 + j% * 4)-(x1 + i% * 4, y1 + 3 + j% * 4), clr + 1ã  END IFã IF POINT(i%, j%) = 15 THENã  LINE (x1 + i% * 4 + 2, y1 + j% * 4)-(x1 + i% * 4 + 2, y1 + 2 + j% * 4), clrã END IFãEND IFããNEXT j%ãNEXT i%ãCOLOR 0: LOCATE 1, 1: PRINT "                    "ãe% = 0: f% = 0: g% = 0ãFOR i% = 1 TO 63ãFOR k = 1 TO 2ãFOR j% = 1 TO wachãNEXT j%ãNEXT kãwacht wachtTijdãIF kl11 = 1 THEN e% = i% ELSE e% = 0ãIF kl12 = 1 THEN f% = i% ELSE f% = 0ãIF kl13 = 1 THEN g% = i% ELSE g% = 0ãh% = clrãkleur h%, e%, f%, g%ãIF kl21 = 1 THEN e% = i% ELSE e% = 0ãIF kl22 = 1 THEN f% = i% ELSE f% = 0ãIF kl23 = 1 THEN g% = i% ELSE g% = 0ãh% = clrãkleur h% + 1, e%, f%, g%ãNEXT i%ãFOR k = 1 TO 100ãFOR j% = 1 TO wachãNEXT j%ãNEXT kãe% = 0: f% = 0: g% = 0ã'FOR i% = 63 TO 1 STEP -1ã'FOR k = 1 TO 2ã'FOR j% = 1 TO wacht STEP 2ã'NEXT j%ã'NEXT kã'IF kl11 = 1 THEN e% = i% ELSE e% = 0ã'IF kl12 = 1 THEN f% = i% ELSE f% = 0ã'IF kl13 = 1 THEN g% = i% ELSE g% = 0ã'kleur 1, e%, f%, g%ã'IF kl21 = 1 THEN e% = i% ELSE e% = 0ã'IF kl22 = 1 THEN f% = i% ELSE f% = 0ã'IF kl23 = 1 THEN g% = i% ELSE g% = 0ã'kleur 2, e%, f%, g%ã'NEXT i%ã'CLSããEND SUBããSUB wacht (tijd)ãSHARED tim2ãFOR i = 1 TO tijd * 50 * tim2ã'randãNEXT iãEND SUBãKurt Kuzba                     THE WOODS (TEXT RPG STYLE) GAMEFidoNet QUIK_BAS Echo          06-20-97 (06:20)       QB, QBasic, PDS        305  13219    THEWOODS.BAS'_|_|_|   THEWOODS.BASã'_|_|_|   A simple game to demonstrate some basic principles.ã'_|_|_|   No warrantee or guarantee given or implied.ã'_|_|_|   PUBLIC DOMAIN   by Kurt Kuzba.  (6/19/1997)ãTYPE character: Pname AS STRING * 32: hits AS LONGã   magic AS LONG: strength AS LONG: gold AS LONGã   Weapon AS INTEGER: experience AS LONG: END TYPEãDIM SHARED Player AS character, Enemy AS characterãRANDOMIZE TIMERãSCREEN 0: WIDTH 80, 25: COLOR 9,0: CLS: LOCATE 3ãLOCATE , 10: PRINT "Your goal is to gain 2,000 experience."ãLOCATE , 10: PRINT "Upon doing this, you will have won.   "ãLOCATE , 10: PRINT "It will NOT, however, be easy.        "ãLOCATE , 10: PRINT "You will meet many malevolent forces. "ãCOLOR 11 : LOCATE 14, 1: INPUT "What is your name? ", plyr$ãIF plyr$ > "" THENã   DO: Player.Pname = plyr$: Player.hits = 30: Player.magic = 0ã      Player.strength = 0: Player.gold = 100: Player.Weapon = 0ã      Player.experience = 0: Gameplayã      PRINT : PRINT "Would you like to play again (YN)? ";ã      DO: k$ = UCASE$(INKEY$): LOOP WHILE INSTR(" YN", k$) < 2ã      PRINT k$: LOOP WHILE k$ = "Y"ãEND IFãPRINT : PRINT "Goodbye, then...": PRINT "Hit any key to continue."ãWHILE INKEY$ > "": WEND: WHILE INKEY$ = "": WEND: SYSTEMãSUB Armory : DIM price(1 TO 8) AS INTEGERã   price(1) = 75: price(2) = 200: price(3) = 400: price(4) = 600ã   price(5) = 900: price(6) = 1500: price(7) = 2500: price(8) = 4000ã   DO: COLOR 10, 0: PRINTã      PRINT " Would you like to Sell or Buy a weapon (S/B/Q)? ";ã      DO: k% = INSTR(" SBQ" + CHR$(13), UCASE$(INKEY$))ã      LOOP WHILE k% < 2ã      PRINT MID$(" SBQQ", k%, 1): PRINTã      SELECT CASE k%ã         CASE 2: money% = Player.Weapon * 30ã            IF money% = 0 THENã               PRINT " You cannot sell your hands here, friend!"ã            ELSEã               PRINT " I will give you"; money%; "gold for your ";ã               PRINT Weapon$(Player.Weapon)ã               PRINT " Is it a deal (Y/N)? ";ã               DO: k% = INSTR(" YN", UCASE$(INKEY$))ã               LOOP WHILE k% < 2ã               PRINT MID$(" YN", k%, 1): PRINTã               IF k% = 2 THENã                  Player.gold = Player.gold + money%ã                  Player.Weapon = 0ã               END IFã            END IFã         CASE 3: PRINT "--==**[  PRICES  ]**==--"ã            FOR t% = 1 TO 8: PRINT STR$(t%); "->";ã               PRINT LEFT$(STR$(price(t%)) + "    ", 6);ã               PRINT Weapon$(t%): NEXTã            PRINT " Hit ENTER to buy nothing"ã            PRINT " You have"; Player.gold; "gold."ã            PRINT "Which would you buy (1-8)? ";ã            DO: k% = INSTR(" 12345678" + CHR$(13), UCASE$(INKEY$))ã            LOOP WHILE k% < 2ã            k% = k% - 1: PRINT MID$("12345678", k%, 1)ã            IF k% < 9 THENã               money% = k% * 90ã               IF money% > Player.gold THENã                  PRINT " You can not afford "; Weapon$(k%)ã               ELSEã                  PRINT " Sold! I hope that "; Weapon$(k%)ã                  PRINT " will serve you well."ã                  Player.gold = Player.gold - money%ã                  Player.Weapon = k%ã               END IFã            END IFã         CASE 4, 5: EXIT SUBã      END SELECTã   LOOP: END SUBããSUB FightMonster : level% = GetLevel%(Player.experience)ã   MakeEnemy level%: menu% = 1ã   WHILE (Player.hits > 0) AND (Enemy.hits > 0)ã      COLOR 4, 0: PRINTã      IF menu% > 0 THENã         PRINT " F-> Fight": PRINT " R-> Run Away"ã         PRINT " S-> Surrender": PRINT " Y-> Your Stats"ã         PRINT " V-> View Enemy"ã      END IFã      COLOR 15, 0: PRINT Player.hits; : COLOR 5, 0: PRINT Player.Pnameã      COLOR 15, 0: PRINT Enemy.hits; : COLOR 5, 0: PRINT Enemy.Pnameã      COLOR 4, 0: PRINT " [ F : R : S : Y : V : ? ]=> ";ã      DO: k% = INSTR(" FRSYV?", UCASE$(INKEY$)): LOOP WHILE k% < 2ã      PRINT MID$(" FRSYV?", k%, 1)ã      level% = GetLevel%(Player.experience)ã      SELECT CASE k%ã         CASE 2ã            PlayerAtt! = 1 + Player.Weaponã            PlayerAtt! = PlayerAtt! * (1 + (Player.strength / 10))ã            PlayerAtt! = 5 + (RND * PlayerAtt! * 5 * level%)ã            PlayerDef! = 1 + (Player.strength / 100)ã            EnemyAtt! = Enemy.Weapon * (1 + (Enemy.strength / 10))ã            EnemyDef! = 1 + (Enemy.strength / 100)ã            IF RND > .5 THENã               PRINT " You move fast and hit first."ã               hits% = PlayerAtt! / EnemyDef!ã               IF hits% > Enemy.hits THEN hits% = Enemy.hitsã               Enemy.hits = Enemy.hits - hits%ã               PRINT " You hit for"; hits%; "points."ã               Player.experience = Player.experience + hits%ã               IF Enemy.hits > 0 THENã                  hits% = EnemyAtt! / PlayerDef!ã                  IF hits% > Player.hits THEN hits% = Player.hitsã                  Player.hits = Player.hits - hits%ã                  PRINT " You are hit for"; hits%; "points."ã               END IFã            ELSEã               PRINT " You move slow and get hit first."ã               hits% = EnemyAtt! / PlayerDef!ã               IF hits% > Player.hits THEN hits% = Player.hitsã               Player.hits = Player.hits - hits%ã               PRINT " You are hit for"; hits%; "points."ã               IF Player.hits > 0 THENã                  hits% = PlayerAtt! / EnemyDef!ã                  IF hits% > Enemy.hits THEN hits% = Enemy.hitsã                  Enemy.hits = Enemy.hits - hits%ã                  Player.experience = Player.experience + hits%ã                  PRINT " You hit for"; hits%; "points."ã               END IFã            END IFã            IF Enemy.hits = 0 THENã               PRINT " You have defeated the "; Enemy.Pnameã               Player.magic = Player.magic + Enemy.magic \ 10ã               Player.gold = Player.gold + Enemy.goldã            END IFã            IF Player.hits = 0 THENã               PRINT " You were defeated by a "; Enemy.Pnameã            END IFã         CASE 3:ã            PRINT " You manage to escape, but all your strength"ã            PRINT " and magic are lost, and some experience points."ã            Player.magic = 0: Player.strength = 0ã            Player.experience = Player.experience * .9ã            EXIT SUBã         CASE 4:ã            PRINT " You give all your gold to the "; Enemy.Pnameã            Player.gold = 0: EXIT SUBã         CASE 5: Stats Playerã         CASE 6: Stats Enemyã         CASE 7: menu% = 1ã      END SELECT: WEND: END SUBããSUB Gameplay : menu% = 1: Stats Playerã   DOã      IF Player.hits > 0 THENã         PRINTã         IF menu% > 0 THENã            COLOR 15, 0: PRINT " W->  Woods": PRINT " H->  Healer"ã            PRINT " A->  Armory": PRINT " S->  Your Stats"ã            PRINT " Q->  Quit Game"ã         END IF: COLOR 11, 0: LOCATE , , 0: menu% = 0ã         PRINT " [ W : H : A : S : Q : ? ]=> ";ã         DO: k% = INSTR(" WHASQ?", UCASE$(INKEY$)): LOOP WHILE k% < 2ã         PRINT MID$(" WHASQ", k%, 1)ã         SELECT CASE k%ã            CASE 2: Woodsã            CASE 3: Healerã            CASE 4: Armoryã            CASE 5: Stats Playerã            CASE 6: PRINT : PRINT "Leave the Woods (YN)? ";ã               DO: k$ = UCASE$(INKEY$)ã               LOOP WHILE INSTR(" YN", k$) < 2ã               PRINT k$: IF k$ = "Y" THEN EXIT DOã            CASE 7: menu% = 1ã         END SELECTã      ELSEã         IF Player.experience < 2000 THENã            PRINT "Sorry... You have perished in battle."ã            FOR t% = 200 TO 150 STEP -1: SOUND t%, .1: NEXTã         ELSEã            PRINT "You have defeated all enemies! You WIN!!"ã            FOR t% = 100 TO 999 STEP 10: SOUND t%, .03: NEXTã            FOR t% = 1000 TO 500 STEP -10: SOUND t%, .03: NEXTã         END IF: PRINT "You had"; Player.experience; "points."ã         EXIT DOã      END IF: LOOP: END SUBãFUNCTION GetLevel% (e&)ã   SELECT CASE e&ã      CASE IS < 100: level% = 1ã      CASE 101 TO 500: level% = 2ã      CASE ELSE: level% = 3ã   END SELECT: GetLevel% = level%: END FUNCTIONãSUB Healer : Totalhits% = Player.experience \ 10 + 30ã   ToHeal% = Totalhits% - Player.hitsã   IF ToHeal% <= 0 THENã      PRINT " You do not require healing!"; CHR$(7)ã   ELSEã      PRINT " Each point costs 3 gold to heal."ã      PRINT " You can afford to heal"; Player.gold \ 3; "hit points."ã      PRINT " You need to heal"; ToHeal%ã      INPUT " How many would you heal? ", heal%ã      IF heal% > ToHeal% THEN heal% = ToHeal%ã      IF heal% > (Player.gold \ 3) THEN heal% = Player.gold \ 3ã      Player.hits = Player.hits + heal%ã      Player.gold = Player.gold - (heal% * 3)ã      PRINT " I have healed you for"; heal%; "hit points."ã   END IF: END SUBãSUB MakeEnemy (l%) : DIM EN(1 TO 3) AS STRING, EP(1 TO 3) AS STRINGã   EP(1) = "Ferocious ": EP(2) = "Snarling ": EP(3) = "Evil "ã   SELECT CASE l%ã      CASE 1: EN(1) = "Squirrel": EN(2) = "Bunny": EN(3) = "Hedgehog"ã      CASE 2: EN(1) = "Bear": EN(2) = "Boar": EN(3) = "Troll"ã      CASE 3: EN(1) = "Demon": EN(2) = "Dragon": EN(3) = "Wizard"ã   END SELECTã   Enemy.Pname = EP(l%) + EN(RND * 2 + 1)ã   Enemy.hits = ((RND * 10 + 20) * l%)ã   Enemy.magic = ((RND * 10 + 10) * l%)ã   Enemy.strength = ((RND * 10 + 10) * l%)ã   Enemy.gold = ((RND * 100 + 100) * l%)ã   Enemy.Weapon = (RND * 2 + l% * 2): END SUBããSUB Stats (P AS character) : COLOR 15, 0: PRINTã   COLOR 15, 1: PRINT " Name       : "; P.Pname;ã   COLOR 15, 0: PRINT : COLOR 15, 1: PRINT " Hits       :";ã   PRINT LEFT$(STR$(P.hits) + SPACE$(32), 33);ã   COLOR 15, 0: PRINT : COLOR 15, 1: PRINT " Magic      :";ã   PRINT LEFT$(STR$(P.magic) + SPACE$(32), 33);ã   COLOR 15, 0: PRINT : COLOR 15, 1: PRINT " Strength   :";ã   PRINT LEFT$(STR$(P.strength) + SPACE$(32), 33);ã   COLOR 15, 0: PRINT : COLOR 15, 1: PRINT " Gold       :";ã   PRINT LEFT$(STR$(P.gold) + SPACE$(32), 33);ã   COLOR 15, 0: PRINT : COLOR 15, 1: PRINT " Weapon     : ";ã   PRINT LEFT$(Weapon$(P.Weapon) + SPACE$(32), 32);ã   COLOR 15, 0: PRINT : COLOR 15, 1: PRINT " Experience :";ã   PRINT LEFT$(STR$(P.experience) + SPACE$(32), 33);ã   COLOR 15, 0: PRINT : END SUBãSUB UseMagic : COLOR 12, 0: PRINT : PRINT : menu% = 1ã   DOã      IF menu% <> 0 THENã         PRINT " H-> Healing Spell   (4 magic)"ã         PRINT " S-> Strength Spell  (9 magic)"ã         PRINT " Q-> Quit this menu"ã      END IF: PRINT " You have"; Player.magic; "spell points."ã      menu% = 0: PRINT " [ H : S : Q : ? ]=> ";ã      DO: k% = INSTR(" HSQ?" + CHR$(13), UCASE$(INKEY$))ã      LOOP WHILE k% < 2: PRINT MID$(" CST?", k%, 1): PRINTã      SELECT CASE k%ã         CASE 2ã            IF Player.magic < 4 THENã               PRINT " You haven't the magic!"ã            ELSEã               m% = 1 + RND * 9ã               Player.magic = Player.magic - 4ã               Player.hits = Player.hits + m%ã               PRINT " Your spell yields"; m%; "hits."ã            END IFã         CASE 3ã            IF Player.magic < 9 THENã               PRINT " You haven't the magic!"ã            ELSEã               m% = 1 + RND * 9ã               Player.magic = Player.magic - 9ã               Player.strength = Player.strength + m%ã               PRINT " Your spell yields"; m%; "strength."ã            END IFã         CASE 4, 6: EXIT SUBã         CASE 5: menu% = 1ã      END SELECT: LOOP: END SUBãSUB WalkInWoodsã   SELECT CASE (RND * 99)ã      CASE 0 TO 9: Player.gold = Player.gold + 1ã         COLOR 14, 0: PRINT "You found a piece of gold!"ã      CASE 10 TO 19: Player.magic = Player.magic + 1ã         COLOR 13, 0: PRINT "A faerie gives you one magic!"ã      CASE 20 TO 29: m% = 1 + RND * 9ã         Player.hits = Player.hits + m%ã         COLOR 9, 0: PRINT "A friendly Elf heals"; m%; "hit";ã         IF m% = 1 THEN PRINT "!":  ELSE PRINT "s!"ã      CASE 30 TO 89ã         COLOR 6, 0: PRINT "The path continues before you."ã      CASE ELSE: FightMonsterã   END SELECT: END SUBãFUNCTION Weapon$ (w%)ã   SELECT CASE w%ã      CASE 0: Weapon$ = "Your Bare Hands"ã      CASE 1: Weapon$ = "A Stick"ã      CASE 2: Weapon$ = "A Quarterstaff"ã      CASE 3: Weapon$ = "A Small Knife"ã      CASE 4: Weapon$ = "A Big Knife"ã      CASE 5: Weapon$ = "A Machete"ã      CASE 6: Weapon$ = "A Shortsword"ã      CASE 7: Weapon$ = "A Longsword"ã      CASE 8: Weapon$ = "A Two-Handed Sword"ã   END SELECT: END FUNCTIONãSUB Woods : menu% = 1ã   DO: level% = GetLevel%(Player.experience)ã      IF Player.experience > 2000 THEN Player.hits = 0ã      COLOR 2, 0: PRINT : IF Player.hits = 0 THEN EXIT SUBã      IF menu% <> 0 THENã         PRINT " C-> Continue": PRINT " T-> Go to Town"ã         PRINT " S-> Cast a Spell": PRINT " Y-> Your Stats"ã      END IF: menu% = 