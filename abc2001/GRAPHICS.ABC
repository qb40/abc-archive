REM This module will return height, width, color depth from JPEG
DIM SHARED mWidth AS LONG
DIM SHARED mHeight AS LONG
DIM SHARED mDepth AS LONG


REM Example
COLOR 14
CLS
LINE INPUT "Input Path and JPG filename "; MyFile$
ReadImageInfo MyFile$
PRINT "Info about "; MyFile$
PRINT USING "Depth ## colors : Width=#### pixels : Height =#### pixels"; mDepth; mWidth; mHeight
SLEEP
END

SUB ReadImageInfo (sFileName AS STRING)
REM This is the sub to call to retrieve information on a file.
DIM iFN AS INTEGER
DIM Ln&
Buf$ = " "
A$ = SPACE$(3)


mWidth = 0
mHeight = 0
mDepth = 0

iFN = FREEFILE
OPEN sFileName FOR BINARY AS iFN
Ln& = LOF(1)
FOR I& = 1 TO Ln&
GET #iFN, I&, Buf$
GET #iFN, I&, A$
REM loop through looking for the byte sequence FF,D8,FF

REM which marks the begining of a JPEG file
IF A$ = CHR$(&HFF) + CHR$(&HD8) + CHR$(&HFF) THEN LinePos& = I&: EXIT FOR
LinePos& = LinePos& = I&
NEXT I&

LinePos& = LinePos& + 2
FOR I& = LinePos& TO Ln&
REM loop through the markers until we find the one
REM starting with FF,C0 which is the block containing the
REM image information
GET #iFN, I&, Buf$
GET #iFN, I&, A$
REM loop until we find the beginning of the next marker
IF ASC(Buf$) = &HFF AND ASC(MID$(A$, 2, 1)) <> &HFF THEN EXIT FOR
LinePos& = LinePos& = I&
NEXT I&


REM move pointer up
LinePos& = LinePos& + 1
FOR I& = LinePos& TO Ln&
GET #iFN, I&, Buf$
GET #iFN, I&, A$
SELECT CASE ASC(Buf$)
CASE &HC0 TO &HC3, &HC5 TO &HC7, &HC9 TO &HCB, &HCD TO &HCF
REM we found the right block
PRINT "Ok for this file"
GOTO ValueIt
CASE ELSE
END SELECT
LinePos& = I&
NEXT I&
CLOSE #iFN
EXIT SUB
ValueIt:
REM If we've gotten this far it is a JPEG and we are ready
REM to grab the information.
REM get the height

GET #iFN, LinePos& + 5, A$
mHeight = 256 * ASC(MID$(A$, 1, 1)) + ASC(MID$(A$, 2, 1))

GET #iFN, LinePos& + 7, A$
REM get the width
mWidth = 256 * ASC(MID$(A$, 1, 1)) + ASC(MID$(A$, 2, 1))
REM get the color depth
GET #iFN, LinePos& + 9, Buf$

mDepth = ASC(Buf$) * 8



END SUB

